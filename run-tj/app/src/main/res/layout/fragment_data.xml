<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.motion.widget.MotionLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layoutDescription="@xml/motion_scene_splash"
        tools:context=".MainActivity"
        tools:ignore="MissingConstraints">

    <TextView
            android:text="数据分析"
            android:layout_width="150dp"
            android:layout_height="40dp" android:id="@+id/textView2"
            android:textColor="#DA888888"
            android:textSize="30sp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp" android:layout_marginStart="4dp"/>
    <ImageView
            android:src="@drawable/data_2"
            android:layout_width="140dp"
            android:layout_height="140dp" android:id="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:layout_marginTop="12dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="84dp"/>
    <ImageView
            android:src="@drawable/data_1"
            android:layout_width="140dp"
            android:layout_height="140dp" android:id="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="84dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="64dp"/>
    <ImageView
            android:src="@drawable/data_3"
            android:layout_width="140dp"
            android:layout_height="140dp" android:id="@+id/imageView4"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp"/>
    <ImageView
            android:src="@drawable/data_4"
            android:layout_width="140dp"
            android:layout_height="140dp" android:id="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"
            android:layout_marginTop="12dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp"/>
    <ImageView
            android:src="@drawable/data_5"
            android:layout_width="140dp"
            android:layout_height="140dp" android:id="@+id/imageView6"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" android:layout_marginTop="8dp"/>
    <TextView
            android:text="跑步计划"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/imageView3" app:layout_constraintStart_toStartOf="@+id/imageView3"
            android:layout_marginTop="64dp" android:layout_marginStart="36dp"/>
    <TextView
            android:text="成绩预测"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/imageView5" app:layout_constraintTop_toTopOf="@+id/imageView5"
            android:layout_marginTop="56dp" android:layout_marginStart="40dp"/>
    <TextView
            android:text="跑姿识别"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/imageView4" app:layout_constraintTop_toTopOf="@+id/imageView4"
            android:layout_marginTop="64dp" android:layout_marginStart="40dp"/>
    <TextView
            android:text="待增加"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            android:textColor="@color/black"
            android:textSize="20sp" app:layout_constraintTop_toTopOf="@+id/imageView2"
            app:layout_constraintStart_toStartOf="@+id/imageView2" android:layout_marginTop="56dp"
            android:layout_marginStart="36dp"/>
    <TextView
            android:text="待增加"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" android:layout_marginTop="68dp"
            android:layout_marginStart="44dp"/>
</androidx.constraintlayout.motion.widget.MotionLayout>